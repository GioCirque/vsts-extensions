{
  "manifestVersion": 1,
  "id": "code-climate",
  "name": "Code Climate",
  "description": "Provides Code Climate interactions",
  "version": "0.2.3",
  "publisher": "palacino-gio",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": ["vso.work", "vso.work_write"],
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "icon.png"
  },
  "files": [
    {
      "path": "task"
    }
  ],
  "contributions": [
    {
      "id": "code-climate",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "task"
      }
    }
  ]
}