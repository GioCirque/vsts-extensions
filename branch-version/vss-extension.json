{
  "manifestVersion": 1,
  "id": "branch-version",
  "name": "Branch Version",
  "description": "Extracts a version number from a branch name",
  "version": "0.2.0",
  "publisher": "palacino-gio",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "icon.png"
  },
  "files": [
    {
      "path": "task"
    }
  ],
  "contributions": [
    {
      "id": "branch-version",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "task"
      }
    }
  ]
